<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>tangconsole.ico</ApplicationIcon>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="components.jpg" />
    <None Remove="tangconsole.png" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="components.jpg">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="tangconsole.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>
</Project>
